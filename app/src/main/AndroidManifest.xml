<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mad.articlenews">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ArticleNews"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ArticleDetailActivity" android:label="@string/article" android:parentActivityName=".ListActivity"></activity>
        <activity android:name=".ListActivity" android:label="List Article" android:parentActivityName=".HomeActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="Create Article"
            android:parentActivityName=".HomeActivity" />
    </application>


</manifest>